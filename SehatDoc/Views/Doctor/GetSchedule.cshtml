@model IEnumerable<DoctorHospitalSchedule>

     @foreach(var schedules in Model)
    {
<div class="card" style="width: 18rem;">
    <div class="card-header">
        @schedules.Doctor.FirstName - @schedules.Doctor.LastName
    </div>
   
        <ul class="list-group list-group-flush">
            <li class="list-group-item">@schedules.Hospitals.HospitalName</li>
            <li class="list-group-item">@schedules.DayOfWeek.ToString()</li>
            <li class="list-group-item">@schedules.StartTime. - @schedules.EndTime</li>
        </ul>
  
</div>
    }